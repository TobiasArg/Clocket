        "preview": "vite preview",
        "test": "vitest run"
    },
    "dependencies": {
        "@tailwindcss/vite": "^4.1.18",
        "@types/react": "^19.2.14",
        "@types/react-dom": "^19.2.3",
        "axios": "^1.13.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-is": "^19.2.4",
        "tailwindcss": "^4.1.18",
        "typescript": "^5.9.3"
    },
    "devDependencies": {
        "@vitejs/plugin-react": "^4.0.0",
        "concurrently": "^9.2.1",
        "jsdom": "^28.1.0",
        "vite": "^5.0.0",
        "vitest": "^4.0.18"
    }
}
